<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.vumobile.ParentActivity"
    android:layout_gravity="center"
    tools:showIn="@layout/app_bar_parent">

    <Button
        android:background="@color/agora_blue"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:text="Celeb Go Live"
        android:id="@+id/btnGoLive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:background="@color/agora_blue"
        android:text="Fan View Live"
        android:id="@+id/btnFanViewLive"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnGoLive"
        android:layout_alignRight="@+id/btnGoLive"
        android:layout_alignEnd="@+id/btnGoLive"
        android:layout_marginTop="57dp" />
</RelativeLayout>
