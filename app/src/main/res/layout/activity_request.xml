<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_request"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/myColorOneBody"
    tools:context="com.vumobile.celeb.ui.RequestActivity">


    <include
        android:id="@+id/toolbar_request"
        layout="@layout/toolbar_request"/>

    <ListView
        android:layout_margin="5dp"
        android:layout_below="@+id/toolbar_request"
        android:id="@+id/listOfRequests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></ListView>


</RelativeLayout>
